{
  "$schema": "https://mintlify.com/schema.json",
  "name": "Advents",
  "logo": {
    "dark": "/assets/brand/dark.svg",
    "light": "/assets/brand/light.svg"
  },
  "favicon": "/assets/favicon.svg",
  "colors": {
    "primary": "#0e0e0e",
    "light": "#d3d3d3",
    "dark": "#0e0e0e",
    "anchors": {
      "from": "#000",
      "to": "#d3d3d3"
    }
  },
  "theme": "prism",
  "layout": "topnav",
  "background": {
    "style": "grid"
  },
  "modeToggle": {
    "default": "light"
  },
  "sidebar": {
    "items": "container"
  },
  "topbar": {
    "style": "default"
  },
  "search": {
    "location": "top",
    "prompt": "Procurar..."
  },
  "rounded": "default",
  "feedback": {
    "suggestEdit": true,
    "raiseIssue": false,
    "thumbsRating": true
  },
  "seo": {
    "indexHiddenPages": true
  },
  "footerSocials": {
    "website": "https://advents.io",
    "linkedin": "https://www.linkedin.com/company/advents-io",
    "github": "https://github.com/advents-io"
  },
  "topbarLinks": [
    {
      "name": "Suporte",
      "url": "/support/contact-us"
    },
    {
      "name": "Cadastrar",
      "url": "https://l.advents.io/cadastro"
    }
  ],
  "topbarCtaButton": {
    "name": "Entrar",
    "url": "https://app.advents.io",
    "style": "roundedRectangle",
    "arrow": true
  },
  "topAnchor": {
    "name": "Documentação"
  },
  "anchors": [
    {
      "name": "Fale conosco",
      "icon": "headset",
      "url": "support"
    },
    {
      "name": "Experimente de graça",
      "icon": "user",
      "url": "https://l.advents.io/cadastro"
    }
  ],
  "navigation": [
    {
      "group": "Documentação",
      "pages": ["introduction", "api-key", "ios-preview-page"]
    },
    {
      "group": "SDKs",
      "pages": [
        {
          "group": "React Native",
          "icon": "react",
          "pages": ["sdks/react-native/introduction", "sdks/react-native/events"]
        }
      ]
    },
    {
      "group": "",
      "pages": ["support/contact-us"]
    }
  ]
}
